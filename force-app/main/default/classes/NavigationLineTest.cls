                '{<[[]]>}<{[{[{[]{()[[[]',
                '[<(<(<(<{}))><([]([]()',
                '<{([([[(<>()){}]>(<<{{',
                '<{([{{}}[<[[[<>{}]]]>[]]'
        };

        Integer validationTotal = 0;
        List<Long> completionScores = new List<Long>();
        for(String thisLine : lines) {
            NavigationLine thisNavigationLine = new NavigationLine(thisLine);
            Integer thisValidationScore = thisNavigationLine.validate();
            validationTotal += thisValidationScore;
            if(thisValidationScore == 0) {
                completionScores.add(thisNavigationLine.getCompletionScore());
            }
        }
        System.debug('Validation total ' + validationTotal);
        completionScores.sort();
        System.debug('Completion total ' + completionScores[completionScores.size()/2]);
    }

